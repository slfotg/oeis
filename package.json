{
  "name": "oeis",
  "displayName": "oeis",
  "description": "Online Encyclopedia of Integer Sequences",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/slfotg/oeis"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oeis.search",
        "title": "OEIS: Sequence Search"
      },
      {
        "command": "oeis.searchSelected",
        "title": "OEIS: Search Selected Text"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "oeis.searchSelected",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.5"
  }
}